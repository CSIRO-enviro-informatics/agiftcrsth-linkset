  dct:modified "2019-04-05"^^xsd:date ;
  dct:contributor _:shane ;
  void:subjectsTarget <https://data.naa.gov.au/def/agift/> ;
  void:objectsTarget <http://linked.data.gov.au/def/crs-th> ;
